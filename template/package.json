    "mini-css-extract-plugin": "^1.6.0",
    "postcss-loader": "^5.3.0",
    "raw-loader": "^4.0.2",
    "register-service-worker": "^1.7.2",
    "secure-ls": "^1.2.6",
    "v-viewer": "^1.5.1",
    "vue": "^2.7.14",
    "vue-moment": "^4.1.0",
    "vue-plugin-load-script": "^1.3.2",
    "vue-router": "^3.5.1",
    "vue2-perfect-scrollbar": "^1.5.2",
    "vuetify": "^2.6.13",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.4",
